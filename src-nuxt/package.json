{
  "name": "nuxt-app",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -Rf nuxt_modules .node_modules .nuxt dist package-lock.json pnpm-lock.yaml yarn.lock",
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "lint": "eslint . && oxlint .",
    "typecheck": "nuxi typecheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build -o storybook-static"
  },
  "dependencies": {
    "@nuxt/content": "^3.12.0",
    "@nuxt/eslint": "^1.15.2",
    "@nuxt/fonts": "^0.14.0",
    "@nuxt/icon": "^2.2.1",
    "@nuxt/scripts": "^0.13.2",
    "@nuxt/test-utils": "^4.0.0",
    "@nuxt/ui": "^4.5.0",
    "@tauri-apps/api": "^2.10.1",
    "@tauri-apps/plugin-opener": "^2.5.3",
    "@unhead/vue": "^2.1.9",
    "better-sqlite3": "^12.6.2",
    "eslint": "^9.39.2",
    "nuxt": "^4.3.1",
    "tailwindcss": "^4.2.1",
    "typescript": "^5.9.3",
    "vue": "^3.5.29",
    "vue-router": "^4.6.4"
  },
  "packageManager": "pnpm@10.28.2",
  "pnpm": {
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "@parcel/watcher",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  },
  "devDependencies": {
    "@storybook/addon-a11y": "^8.6.17",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@storybook/vue3-vite": "^8.6.17",
    "eslint-config-prettier": "^10.1.8",
    "oxlint": "^1.50.0",
    "prettier": "^3.8.1",
    "storybook": "^8.6.17",
    "vitest": "^4.0.18"
  }
}
